{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/js/*",
      "/css/*"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html",
      "exclude": [
        "/images/*.{png,jpg,gif,svg}",
        "/css/*",
        "/js/*"
      ]
    }
  ]
}
